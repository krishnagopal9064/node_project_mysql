<%-include('./header2')%>
<div class="container">  
<table class="table table-striped">
    <thead>
        <tr>
            <td>User Id</td>
            <td>User Name</td>
            <td>Contact No.</td>
            <td>Action</td>
        </tr>
    </thead>
    <tbody>
        <% for(let data of results) { %>
        <tr>
            <td><%= data.user_id %></td>
            <td><%= data.name %></td>
            <td><%= data.contact %></td>
            <td> <button class="btn btn-primary"> <a href="/edituser/<%= data.user_id %>" style="color:white; text-decoration:none;"> Edit </a> </button>
                <button class="btn btn-danger"> <a href="/deleteuser/<%= data.user_id %>" style="color:white; text-decoration:none;"> Delete </a> </button>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>
</div> 
    

      <%-include('./footer')%>